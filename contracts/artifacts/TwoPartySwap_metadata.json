		"userdoc": {
			"events": {
				"AssetEscrowed(address,uint256,address,address,uint256,uint256)": {
					"notice": "AssetEscrowed: this event should emit  when the assetEscrower successfully escrows the asset"
				},
				"AssetRedeemed(address,uint256,address,address,uint256,uint256)": {
					"notice": "AssetRedeemed: this event should emit when the assetEscrower successfully escrows the asset"
				},
				"AssetRefunded(address,uint256,address,address,uint256,uint256)": {
					"notice": "AssetRefunded: this event should emit when the counterparty breaks the protocol  and the assetEscrower succesffully gets their asset refunded"
				},
				"PremiumEscrowed(address,uint256,address,address,uint256,uint256)": {
					"notice": "PremiumEscrowed: this event should emit when the premiumEscrower successfully escrows the premium"
				},
				"PremiumRedeemed(address,uint256,address,address,uint256,uint256)": {
					"notice": "PremiumRedeemed: this event should emit when the counterparty breaks the protocol and the assetEscrower redeems the  premium for breaking the protocol "
				},
				"PremiumRefunded(address,uint256,address,address,uint256,uint256)": {
					"notice": "PremiumRefunded: this event should emit when the premiumEscrower successfully gets their premium refunded"
				},
				"SetUp(address,address,uint256,uint256,uint256,uint256,uint256,uint256)": {
					"notice": "SetUp: this event should emit when a swap is successfully setup."
				}
			},
			"kind": "user",
			"methods": {
				"escrowAsset(bytes32)": {
					"notice": "The asset escrower has to escrow their premium for the protocol to succeed"
				},
				"escrowPremium(bytes32)": {
					"notice": "The premium escrower has to escrow their premium for the protocol to succeed."
				},
				"redeemAsset(bytes32,bytes32)": {
					"notice": "redeemAsset redeems the asset for the new owner"
				},
				"redeemPremium(bytes32)": {
					"notice": "redeemPremium allows a party to redeem the counterparty's premium should the swap fail"
				},
				"refundAsset(bytes32)": {
					"notice": "refundAsset refunds the asset to its original owner should the swap fail"
				},
				"refundPremium(bytes32)": {
					"notice": "refundPremium refunds the premiumEscrower's premium should the swap succeed"
				},
				"setup(uint256,uint256,address,address,address,bytes32,uint256,bool,uint256)": {
					"notice": "setup is called to initialize an instance of a swap in this contract.  Due to storage constraints, the various parts of the swap are spread  out between the three different mappings above: swaps, assets,  and premiums."
				},
				"swaps(bytes32)": {
					"notice": "Mappings that store our swap details. This contract stores multiple swaps; you can access information about a specific swap by using its hashLock as the key to the appropriate mapping."
				}
			},
			"version": 1
		}
